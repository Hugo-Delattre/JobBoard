{
    "root": true,
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "extends": [
         "eslint:recommended",
         "prettier",
         "plugin:react/recommended"
    ],
    "env": {
         "browser": true,
         "es2021": true,
         "node": true
    },
    "rules": {
         "no-console": "error"
    }
}